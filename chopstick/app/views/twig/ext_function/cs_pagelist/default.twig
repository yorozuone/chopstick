<div id="{{ id }}" class="{{ class }}">
    {% for v in drec_pagelist %}
        {% if v.external_link == '' %}
            <a href="{{ cs_urlp(v.page_id) }}"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> {{ v.page_title }}</a>
        {% else %}
            <a href="{{ v.external_link }}"><span class="glyphicon glyphicon-link" aria-hidden="true"></span> {{ v.page_title }}</a>
        {% endif %}
        <br>
    {% endfor %}
</div>
