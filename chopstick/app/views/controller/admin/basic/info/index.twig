{% extends "/controller/admin/default.twig" %}

{% block area_body %}

    <ol class="breadcrumb">
        <li><a href="{{ cs_url('admin/menu') }}">Home</a></li>
        <li class="active">情報</li>
    </ol>

    <div class="page-header">
        <h1>情報</h1>
    </div>

    <h2>PHP 情報</h2>
    <table class="table table-striped">
        <tr>
            <td>PHP バージョン</td>
            <td>{{ phpversion }}</td>
        </tr>
    </table>

    <h2>グローバル</h2>
    <pre>{{ globalvars_dump }}</pre>

    <h2>セッション</h2>
    <pre>{{ session_dump }}</pre>

    <h2>ACL</h2>
    <pre>{{ acl_dump|raw }}</pre>

{% endblock %}